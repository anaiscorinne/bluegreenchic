<div class="container text-in-parallax">
  <div class="section ">
    <div class="row ">
      <div class="col s12 m12">
            <h5 class="center hp-h5-title">Add a new Brand</h5>
              <%= form_for [@brand] do |f| %>

                <%= f.label :name %>
                <%= f.text_field :name %>
                <br>
                <%= f.label :description %>
                <%= f.text_area :description %>
                <br>
                <%= f.label :website %>
                <%= f.text_field :website %>
                <br>
                <%= f.label :instagram %>
                <%= f.text_field :instagram %>
                <br>
                <%= f.submit "Save" %>
                <br>
                <%= link_to("Cancel", brands_path) %>
                <br>
                <%= link_to("Home", "/") %>
              <% end %>

        </div>
    </div>
  </div>
</div>


