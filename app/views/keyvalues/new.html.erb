<%= render "layouts/nav" %>

<div class="container">
<div class="row ">
  <div class="col s12 m12">
        <br><h5 class="center hp-h5-title">Add a new Value</h5><hr><br>
              <%= form_for [@keyvalue] do |f| %>

                <%= f.label :name %>
                <%= f.text_field :name, autofocus: true %>
                <br>
                <%= f.label :description %>
                <%= f.text_area :description %>
                <br>
                <%= f.label :icon %>
                <%= f.text_field :icon %>
                <br>
                <%= f.submit "Save" %>
                <br>
                <%= link_to("Cancel", brands_path) %>
                <br>
                <%= link_to("Home", "/") %>
              <% end %>

        </div>
    </div>
  </div>



